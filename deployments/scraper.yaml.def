apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: scraper
spec:
  replicas: 1
  template:
    metadata:
      labels:
        app: scraper
    spec:
      containers:
      - name: scraper
        image: gcr.io/eveinc-158804/killmailscraper:master_latest
        imagePullPolicy: Always
        resources:
          requests:
            memory: "256Mi"
            cpu: "50m"
